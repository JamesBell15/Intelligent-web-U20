PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbprop: <http://dbpedia.org/property/>
PREFIX dbr: <http://dbpedia.org/resource>

select distinct ?label ?abstract ?species ?genus ?thumbnail
where {
    <http://dbpedia.org/resource/Securities_Laws_(Amendment)_Act,_2014> rdf:type dbo:Bird;
        rdfs:label ?label;
        dbo:abstract ?abstract.
    OPTIONAL{

        <http://dbpedia.org/resource/Securities_Laws_(Amendment)_Act,_2014> dbp:genus ?genus.}
    OPTIONAL{
        <http://dbpedia.org/resource/Securities_Laws_(Amendment)_Act,_2014e> dbp:species ?species.}
    OPTIONAL{
        <http://dbpedia.org/resource/Securities_Laws_(Amendment)_Act,_2014> dbo:thumbnail ?thumbnail.}
    FILTER(langMatches(lang(?label), "en" ))
    FILTER(langMatches(lang(?abstract), "en" ))
} LIMIT 15